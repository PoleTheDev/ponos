{
  "name": "ponos",
  "version": "0.9.0",
  "description": "An opinionated queue based worker server for node.",
  "main": "index.js",
  "scripts": {
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --recursive --reporter spec test/unit && npm run coverage-check",
    "coverage-check": "istanbul check-coverage && echo 'Coverage check successful!'",
    "functional": "mocha --recursive --reporter spec test/functional",
    "lint": "eslint .",
    "test": "npm run lint && npm run unit && npm run functional",
    "unit": "mocha --bail --recursive --reporter spec test/unit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Runnable/ponos.git"
  },
  "keywords": [
    "worker",
    "queue",
    "rabbit",
    "job",
    "message"
  ],
  "author": "Ryan Sandor Richards <sandor.richards@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Runnable/ponos/issues"
  },
  "homepage": "https://github.com/Runnable/ponos",
  "dependencies": {
    "101": "^1.0.0",
    "bluebird": "^2.10.1",
    "bunyan": "^1.5.1",
    "error-cat": "^1.3.4",
    "loadenv": "^1.1.0",
    "runnable-hermes": "^5.3.0"
  },
  "devDependencies": {
    "async": "^1.4.2",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.5.1",
    "istanbul": "^0.3.20",
    "mocha": "^2.3.3",
    "sinon": "^1.17.0"
  }
}
